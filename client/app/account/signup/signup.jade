.container
  .col-sm-12
    h1 Sign up
  .col-sm-12
    form.form.signup-form(
        name='form',
        on-valid-submit="loginOauth('stripe')"
        )
      .row
        .col-sm-6
          .form-group
            my-errors(field="fields.firstName",form="form")
              my-field(field="fields.firstName",data="user")
          
          .form-group
            my-errors(field="fields.lastName",form="form")
              my-field(field="fields.lastName",data="user")
          .form-group
            my-errors(field="fields.firstName",form="form")
              my-field(field="fields.firstName",data="user")
          .form-group
            my-errors(field="fields.email",form="form")
              my-field(field="fields.email",data="user")
          .form-group
            my-errors(field="fields.phoneNumber",form="form")
              my-field(field="fields.phoneNumber",data="user")
          .form-group
            my-errors(field="fields.dob",form="form")
              my-field(field="fields.dob",data="user")
        .col-sm-6
          .form-group
            my-errors(field="fields.businessName",form="form")
              my-field(field="fields.businessName",data="user")
          .form-group
            my-errors(field="fields.productDescription",form="form")
              my-field(field="fields.productDescription",data="user")
          .form-group
            my-errors(field="fields.businessType",form="form")
              my-field(field="fields.businessType",data="user")
          .form-group
            my-errors(field="fields.streetAddress",form="form")
              my-field(field="fields.streetAddress",data="user")
          .row
            .col-sm-5
              my-errors(field="fields.city",form="form")
                my-field(field="fields.city",data="user")
            .col-sm-3
              my-errors(field="fields.zip",form="form")
                my-field(field="fields.zip",data="user")
            .col-sm-4
              my-errors(field="fields.state",form="form")
                my-field(field="fields.state",data="user")
          button.stripe-connect.dark(type="submit")
            span Connect with Stripe

